FUNCTION alt_u_div_p2f (denominator[22..0], numerator[22..0])
RETURNS ( quotient[22..0], remainder[22..0]);
FUNCTION lpm_abs_cp9 (data[22..0])
RETURNS ( overflow, result[22..0]);

--synthesis_resources = lut 391 
SUBDESIGN abs_divider_5dg
( 
	denominator[22..0]	:	input;
	numerator[22..0]	:	input;
	quotient[22..0]	:	output;
	remainder[22..0]	:	output;
) 
VARIABLE 
	divider : alt_u_div_p2f;
	my_abs_den : lpm_abs_cp9;
	my_abs_num : lpm_abs_cp9;
	compl_add_quot_result_int[23..0]	:	WIRE;
	compl_add_quot_cin	:	WIRE;
	compl_add_quot_dataa[22..0]	:	WIRE;
	compl_add_quot_datab[22..0]	:	WIRE;
	compl_add_quot_result[22..0]	:	WIRE;
	compl_add_rem_result_int[23..0]	:	WIRE;
	compl_add_rem_cin	:	WIRE;
	compl_add_rem_dataa[22..0]	:	WIRE;
	compl_add_rem_datab[22..0]	:	WIRE;
	compl_add_rem_result[22..0]	:	WIRE;
	diff_signs	: WIRE;
	gnd_wire	: WIRE;
	neg_quot[22..0]	: WIRE;
	neg_rem[22..0]	: WIRE;
	norm_den[22..0]	: WIRE;
	norm_num[22..0]	: WIRE;
	num_sign	: WIRE;
	protect_quotient[22..0]	: WIRE;
	protect_remainder[22..0]	: WIRE;
	vcc_wire	: WIRE;

BEGIN 
	divider.denominator[] = norm_den[];
	divider.numerator[] = norm_num[];
	my_abs_den.data[] = denominator[];
	my_abs_num.data[] = numerator[];
	compl_add_quot_result_int[] = (compl_add_quot_dataa[], compl_add_quot_cin) + (compl_add_quot_datab[], compl_add_quot_cin);
	compl_add_quot_result[] = compl_add_quot_result_int[23..1];
	compl_add_quot_cin = vcc_wire;
	compl_add_quot_dataa[] = (! protect_quotient[]);
	compl_add_quot_datab[] = ( gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire);
	compl_add_rem_result_int[] = (compl_add_rem_dataa[], compl_add_rem_cin) + (compl_add_rem_datab[], compl_add_rem_cin);
	compl_add_rem_result[] = compl_add_rem_result_int[23..1];
	compl_add_rem_cin = vcc_wire;
	compl_add_rem_dataa[] = (! protect_remainder[]);
	compl_add_rem_datab[] = ( gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire, gnd_wire);
	diff_signs = (numerator[22..22] $ denominator[22..22]);
	gnd_wire = B"0";
	neg_quot[] = compl_add_quot_result[];
	neg_rem[] = compl_add_rem_result[];
	norm_den[] = my_abs_den.result[];
	norm_num[] = my_abs_num.result[];
	num_sign = numerator[22..22];
	protect_quotient[] = divider.quotient[];
	protect_remainder[] = divider.remainder[];
	quotient[] = ((protect_quotient[] & (! diff_signs)) # (neg_quot[] & diff_signs));
	remainder[] = ((protect_remainder[] & (! num_sign)) # (neg_rem[] & num_sign));
	vcc_wire = B"1";
END;
